{
  "name": "yongheui",
  "version": "0.0.0",
  "description": "SQKB BigData UI",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --config build/webpack.config.base.js",
    "bootstrap": "yarn --frozen-lockfile && npx lerna bootstrap",
    "build": "yarn bootstrap && yarn clean:lib && yarn build:esm-bundle && yarn build:lib && yarn build:utils && yarn build:theme",
    "clean:lib": "rimraf lib && rimraf es",
    "build:lib": "cross-env LIBMODE=core webpack --config ./build/webpack.config.js",
    "build:esm-bundle": "rollup --config ./build/rollup.config.bundle.js && yarn build:type",
    "build:type": "node build/gen-type.js",
    "build:utils": "cross-env BABEL_ENV=utils babel packages/utils --extensions .ts --out-dir lib/utils",
    "build:theme": "rimraf packages/theme-default/lib && gulp build --gulpfile packages/theme-default/gulpfile.js && cp-cli packages/theme-default/lib lib/theme-default && rimraf packages/theme-default/lib",
    "website-build": "rimraf website-dist && cross-env NODE_ENV=production webpack --config ./website/webpack.config.js",
    "website-dev": "webpack-dev-server --config ./website/webpack.config.js",
    "website-dev:play": "cross-env PLAY_ENV=true yarn website-dev"
  },
  "keywords": [
    "Vue",
    "components"
  ],
  "author": "dafei",
  "license": "GPL",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.161",
    "@vue/babel-plugin-jsx": "^1.0.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/component-compiler-utils": "^3.2.0",
    "algoliasearch": "^4.4.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-vue": "^2.0.2",
    "cp-cli": "2.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "cz-conventional-changelog": "^3.3.0",
    "element-plus": "^1.0.2-beta.28",
    "file-loader": "^6.0.0",
    "file-save": "^0.2.0",
    "highlight.js": "^10.4.1",
    "html-webpack-plugin": "^4.5.1",
    "lerna": "^3.22.1",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^0.11.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-vue": "^6.0.0-beta.11",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "throttle-debounce": "2.3.0",
    "transliteration": "^2.1.11",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.0",
    "vue": "^3.0.5",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "mitt": "^2.1.0"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
